@{
	Layout = null;
}

<!DOCTYPE html>
<html>
@await Component.InvokeAsync("_LayoutHeaderPartialComponent")
<body>
	<div class="wrapper">
		@await Component.InvokeAsync("_LayoutNavBarComponentPartial")



		@await Component.InvokeAsync("_LayoutSidebarComponentPartial")
		<div class="main-panel">
			@RenderBody()
			@await Component.InvokeAsync("_LayoutFooterPartialComponent")
		</div>
	</div>
	>

	@await Component.InvokeAsync("_LayoutScriptComponentPartial")
</body>

</html>

